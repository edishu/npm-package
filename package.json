{
  "name": "@edishu/npm-package-demo",
  "main": "dist/index.js",
  "version": "1.0.0",
  "description": "A demo package for Total TypeScript",
  "keywords": [
    "demo",
    "typescript"
  ],
  "homepage": "https://github.com/edishu/npm-package",
  "bugs": {
    "url": "https://github.com/edishu/npm-package/issues"
  },
  "author": "Udayan Maurya <edishumaurya@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edishu/npm-package.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.27.9",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "scripts": {
    "lint": "tsc",
    "test": "vitest run",
    "dev": "vitest",
    "build": "tsup",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack .",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run lint && npm run test",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  }
}
